# The train/test net protocol buffer definition
net: "src/RankIQA/tid2013/train_vgg.prototxt"

test_iter: 1

test_interval: 1000
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 10000
display: 100
max_iter: 50000
momentum: 0.9
solver_type: SGD
weight_decay: 0.0005
average_loss: 500
snapshot: 10000
snapshot_prefix: "models/rank_tid2013/my_siamese"
solver_mode: GPU
